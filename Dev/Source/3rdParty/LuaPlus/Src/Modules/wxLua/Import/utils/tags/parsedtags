///////////////////////////////////////////////////////////////////////////////
// wxSocketBase

%include "\socket.h"
%class %noclassinfo wxSocketBase, wxObject
    wxSocketBase()
    wxSocketBase(wxSocketFlags flags, wxSocketType type)
    virtual ~wxSocketBase()
    void Init()
    bool Destroy()
    bool Ok() const
    bool Error() const
    bool IsConnected() const
    bool IsData()
    bool IsDisconnected() const
    wxUint32 LastCount() const
    wxSocketError LastError() const
    void SaveState()
    void RestoreState()
    virtual bool GetLocal(wxSockAddress& addr_man) const
    virtual bool GetPeer(wxSockAddress& addr_man) const
    virtual bool  Close()
    wxSocketBase& Discard()
    wxSocketBase& Peek(void* buffer, wxUint32 nbytes)
    wxSocketBase& Read(void* buffer, wxUint32 nbytes)
    wxSocketBase& ReadMsg(void *buffer, wxUint32 nbytes)
    wxSocketBase& Unread(const void *buffer, wxUint32 nbytes)
    wxSocketBase& Write(const void *buffer, wxUint32 nbytes)
    wxSocketBase& WriteMsg(const void *buffer, wxUint32 nbytes)
    void InterruptWait()
    bool Wait(long seconds = -1, long milliseconds = 0)
    bool WaitForRead(long seconds = -1, long milliseconds = 0)
    bool WaitForWrite(long seconds = -1, long milliseconds = 0)
    bool WaitForLost(long seconds = -1, long milliseconds = 0)
    wxSocketFlags GetFlags() const
    void SetFlags(wxSocketFlags flags)
    void SetTimeout(long seconds)
    bool GetOption(int level, int optname, void *optval, int *optlen)
    bool SetOption(int level, int optname, const void *optval, int optlen)
    wxUint32 GetLastIOSize() const
    void *GetClientData() const
    void SetClientData(void *data)
    void SetEventHandler(wxEvtHandler& handler, int id = wxID_ANY)
    void SetNotify(wxSocketEventFlags flags)
    void Notify(bool notify)
    static bool IsInitialized()
    static bool Initialize()
    static void Shutdown()
    void OnRequest(wxSocketNotify notify)
    bool IsNoWait() const
    wxSocketType GetType() const
%endclass

