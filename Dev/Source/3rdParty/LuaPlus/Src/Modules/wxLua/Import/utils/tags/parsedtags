///////////////////////////////////////////////////////////////////////////////
// wxListItem

%include "\listbase.h"
%class %noclassinfo wxListItem, wxObject
    wxListItem()
    wxListItem(const wxListItem& item)
    virtual ~wxListItem()
    void Clear()
    void ClearAttributes()
    void SetMask(long mask)
    void SetId(long id)
    void SetColumn(int col)
    void SetState(long state)
    void SetStateMask(long stateMask)
    void SetText(const wxString& text)
    void SetImage(int image)
    void SetData(long data)
    void SetData(void *data)
    void SetWidth(int width)
    void SetAlign(wxListColumnFormat align)
    void SetTextColour(const wxColour& colText)
    void SetBackgroundColour(const wxColour& colBack)
    void SetFont(const wxFont& font)
    long GetMask() const
    long GetId() const
    int GetColumn() const
    long GetState() const
    const wxString& GetText() const
    int GetImage() const
    wxUIntPtr GetData() const
    int GetWidth() const
    wxListColumnFormat GetAlign() const
    wxListItemAttr *GetAttributes() const
    bool HasAttributes() const
    wxColour GetTextColour() const
    wxColour GetBackgroundColour() const
    wxFont GetFont() const
    operator long() const
    %member long            m_mask
    %member long            m_itemId
    %member int             m_col
    %member long            m_state
    %member long            m_stateMask
    %member wxString        m_text
    %member int             m_image
    %member wxUIntPtr       m_data
    %member int             m_format
    %member int             m_width
    %member int             m_miniImage
%endclass

