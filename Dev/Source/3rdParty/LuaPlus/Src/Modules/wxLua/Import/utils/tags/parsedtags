// \except.h
// *****************************************************************

%define wxTRY try
%define wxCATCH_ALL(code) catch ( ... )
%define wxTRY
%define wxCATCH_ALL(code)

// *****************************************************************
// \fdrepdlg.h
// *****************************************************************

%define wxGenericFindReplaceDialog wxFindReplaceDialog

