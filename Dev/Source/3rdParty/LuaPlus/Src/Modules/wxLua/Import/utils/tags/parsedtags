    const wxString& WXUNUSED(item),wxChar *WXUNUSED(data),int WXUNUSED(size),wxIPCFormat WXUNUSED(format) )
    wxChar *WXUNUSED(data),int WXUNUSED(size),wxIPCFormat WXUNUSED(format) )
    int WXUNUSED(size),wxIPCFormat WXUNUSED(format) )
    wxIPCFormat WXUNUSED(format) )
