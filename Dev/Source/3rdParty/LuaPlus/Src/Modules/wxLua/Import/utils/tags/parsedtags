    wxPROP_ENUM_STORE_LONG
    wxPROP_DONT_STREAM
%end

%enum wxTypeKind
    wxT_VOID
    wxT_BOOL
    wxT_CHAR
    wxT_UCHAR
    wxT_INT
    wxT_UINT
    wxT_LONG
    wxT_ULONG
    wxT_FLOAT
    wxT_DOUBLE
    wxT_STRING
    wxT_SET
    wxT_ENUM
    wxT_CUSTOM
    wxT_LAST_SIMPLE_TYPE_KIND
    wxT_OBJECT_PTR
    wxT_OBJECT
    wxT_COLLECTION
    wxT_DELEGATE
    wxT_LAST_TYPE_KIND
%end

