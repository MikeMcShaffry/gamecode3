    virtual void HandleEvent(wxEvtHandler *handler,wxEventFunction func,wxEvent& event) const
    virtual void ProcessPendingEvents()
    virtual bool Yield(bool WXUNUSED(onlyIfNeeded) = false)
    static bool CheckBuildOptions(const char *optionsSignature,const char *componentName)
    wxDEPRECATED( static bool CheckBuildOptions(const wxBuildOptions& buildOptions) )
    static void SetInitializerFunction(wxAppInitializerFunction fn)
    static wxAppInitializerFunction GetInitializerFunction()
    static wxAppConsole *GetInstance()
    static void SetInstance(wxAppConsole *app)
    %member int      argc
    %member wxChar **argv
%endclass

