    const wxString& WXUNUSED(item),int *WXUNUSED(size),wxIPCFormat WXUNUSED(format) )
    int *WXUNUSED(size),wxIPCFormat WXUNUSED(format) )
    wxIPCFormat WXUNUSED(format) )
