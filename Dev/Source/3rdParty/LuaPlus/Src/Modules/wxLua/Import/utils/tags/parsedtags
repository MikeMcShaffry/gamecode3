%define wxCRITSECT_IS_MUTEX 1
%define wxCRITSECT_INLINE inline
%define wxCRITSECT_IS_MUTEX 0
%define wxCRITSECT_INLINE
%define wxENTER_CRIT_SECT(cs)   (cs).Enter()
%define wxLEAVE_CRIT_SECT(cs)   (cs).Leave()
%define wxCRIT_SECT_DECLARE(cs) static wxCriticalSection cs
%define wxCRIT_SECT_DECLARE_MEMBER(cs) wxCriticalSection cs
%define wxCRIT_SECT_LOCKER(name, cs)  wxCriticalSectionLocker name(cs)
%define wxENTER_CRIT_SECT(cs)
%define wxLEAVE_CRIT_SECT(cs)
%define wxCRIT_SECT_DECLARE(cs)
%define wxCRIT_SECT_DECLARE_MEMBER(cs)
%define wxCRIT_SECT_LOCKER(name, cs)

typedef char wxCritSectBuffer[40]
typedef char wxCritSectBuffer[24]
typedef unsigned long long wxThreadIdType
typedef unsigned long wxThreadIdType
typedef void *ExitCode

%enum 
    WXTHREAD_MIN_PRIORITY
    WXTHREAD_DEFAULT_PRIORITY
    WXTHREAD_MAX_PRIORITY
%end

%enum wxThreadError
    wxTHREAD_NO_ERROR
    wxTHREAD_NO_RESOURCE
    wxTHREAD_RUNNING
    wxTHREAD_NOT_RUNNING
    wxTHREAD_KILLED
    wxTHREAD_MISC_ERROR
%end

%enum wxCondError
    wxCOND_NO_ERROR
    wxCOND_INVALID
    wxCOND_TIMEOUT
    wxCOND_MISC_ERROR
%end

%enum wxMutexType
    wxMUTEX_DEFAULT
    wxMUTEX_RECURSIVE
%end

%enum wxSemaError
    wxSEMA_NO_ERROR
    wxSEMA_INVALID
    wxSEMA_BUSY
    wxSEMA_TIMEOUT
    wxSEMA_OVERFLOW
    wxSEMA_MISC_ERROR
%end

%enum wxMutexError
    wxMUTEX_NO_ERROR
    wxMUTEX_INVALID
    wxMUTEX_DEAD_LOCK
    wxMUTEX_BUSY
    wxMUTEX_UNLOCKED
    wxMUTEX_MISC_ERROR
%end

%enum wxThreadKind
    wxTHREAD_DETACHED
    wxTHREAD_JOINABLE
%end

