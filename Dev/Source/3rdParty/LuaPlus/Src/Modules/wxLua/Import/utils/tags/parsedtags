    bool              UnrefLib()
    void  RefObj()
    void  UnrefObj()
    bool  IsLoaded() const
    void  Unload()
%endclass

///////////////////////////////////////////////////////////////////////////////
// wxPluginManager

%include "\dynload.h"
%class %noclassinfo wxPluginManager
    static wxPluginLibrary    *LoadLibrary( const wxString &libname,int flags = wxDL_DEFAULT )
    static bool                UnloadLibrary(const wxString &libname)
    wxDEPRECATED( static wxPluginLibrary *GetObjectFromHandle(wxDllType handle) )
    wxPluginManager() : m_entry(NULL)
    wxPluginManager(const wxString &libname, int flags = wxDL_DEFAULT)
    ~wxPluginManager()
    bool   Load(const wxString &libname, int flags = wxDL_DEFAULT)
    void   Unload()
    bool   IsLoaded() const
    void  *GetSymbol(const wxString &symbol, bool *success = 0)
    static void CreateManifest()
    static void ClearManifest()
%endclass

