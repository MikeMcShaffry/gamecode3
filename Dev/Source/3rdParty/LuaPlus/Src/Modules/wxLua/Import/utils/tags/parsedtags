%define  wxCONFIG_CASE_SENSITIVE       0
%define   wxCONFIG_PATH_SEPARATOR     _T('/')
%define   wxCONFIG_IMMUTABLE_PREFIX   _T('!')
%define wxUSE_CONFIG_NATIVE 1
%define wxConfig  wxRegConfig
%define wxConfig  wxFileConfig

