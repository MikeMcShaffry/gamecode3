// *****************************************************************
// \socket.h
// *****************************************************************

typedef GSocketEventFlags wxSocketEventFlags
typedef int wxSocketFlags
typedef void (wxEvtHandler::*wxSocketEventFunction)(wxSocketEvent&)

%enum 
    wxSOCKET_INPUT_FLAG
    wxSOCKET_OUTPUT_FLAG
    wxSOCKET_CONNECTION_FLAG
    wxSOCKET_LOST_FLAG
    wxSOCKET_NONE
    wxSOCKET_NOWAIT
    wxSOCKET_WAITALL
    wxSOCKET_BLOCK
    wxSOCKET_REUSEADDR
%end

%enum wxSocketNotify
    wxSOCKET_INPUT
    wxSOCKET_OUTPUT
    wxSOCKET_CONNECTION
    wxSOCKET_LOST
%end

%enum wxSocketType
    wxSOCKET_UNINIT
    wxSOCKET_CLIENT
    wxSOCKET_SERVER
    wxSOCKET_BASE
    wxSOCKET_DATAGRAM
%end

%enum wxSocketError
    wxSOCKET_NOERROR
    wxSOCKET_INVOP
    wxSOCKET_IOERR
    wxSOCKET_INVADDR
    wxSOCKET_INVSOCK
    wxSOCKET_NOHOST
    wxSOCKET_INVPORT
    wxSOCKET_WOULDBLOCK
    wxSOCKET_TIMEDOUT
    wxSOCKET_MEMERR
    wxSOCKET_DUMMY
%end

