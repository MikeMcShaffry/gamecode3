// wxZipEntry

%include "\zipstrm.h"
%class %noclassinfo wxZipEntry, wxArchiveEntry
    wxZipEntry(const wxString& name = wxEmptyString,const wxDateTime& dt = wxDateTime::Now(),wxFileOffset size = wxInvalidOffset)
    virtual ~wxZipEntry()
