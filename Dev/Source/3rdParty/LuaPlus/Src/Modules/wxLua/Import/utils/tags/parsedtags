    wxCONFIG_USE_LOCAL_FILE
    wxCONFIG_USE_GLOBAL_FILE
    wxCONFIG_USE_RELATIVE_PATH
    wxCONFIG_USE_NO_ESCAPE_CHARACTERS
%end

