    bool Create( wxWindow *parent,wxWindowID winid,const wxString& title,const wxPoint& pos = wxDefaultPosition,const wxSize& size = wxDefaultSize,long style = wxDEFAULT_FRAME_STYLE | wxVSCROLL | wxHSCROLL,const wxString& name = wxFrameNameStr )
    wxMenu* GetWindowMenu() const
    void SetWindowMenu(wxMenu* pMenu)
    virtual void SetMenuBar(wxMenuBar *pMenuBar)
    void SetChildMenuBar(wxGenericMDIChildFrame *pChild)
    virtual bool ProcessEvent(wxEvent& event)
    wxGenericMDIChildFrame *GetActiveChild() const
    void SetActiveChild(wxGenericMDIChildFrame* pChildFrame)
    wxGenericMDIClientWindow *GetClientWindow() const
    virtual wxGenericMDIClientWindow *OnCreateClient()
    virtual void Cascade()
    virtual void Tile()
    virtual void ArrangeIcons()
    virtual void ActivateNext()
    virtual void ActivatePrevious()
%endclass

