    %member short        decimalDigits
    %member short        numPrecRadix
    %member short        nullable
    %member wxChar       remarks[254+1]
    %member int          dbDataType
    %member int          PkCol
    %member wxChar       PkTableName[DB_MAX_TABLE_NAME_LEN+1]
    %member int          FkCol
    %member wxChar       FkTableName[DB_MAX_TABLE_NAME_LEN+1]
    %member wxDbColFor  *pColFor
    wxDbColInf()
    ~wxDbColInf()
    bool Initialize()
%endclass

