// *****************************************************************
// \volume.h
// *****************************************************************

typedef wxFSVolumeBase wxFSVolume

%enum wxFSVolumeKind
    wxFS_VOL_FLOPPY
    wxFS_VOL_DISK
    wxFS_VOL_CDROM
    wxFS_VOL_DVDROM
    wxFS_VOL_NETWORK
    wxFS_VOL_OTHER
    wxFS_VOL_MAX
%end

%enum 
    wxFS_VOL_MOUNTED
    wxFS_VOL_REMOVABLE
    wxFS_VOL_READONLY
    wxFS_VOL_REMOTE
%end

%enum wxFSIconType
    wxFS_VOL_ICO_SMALL
    wxFS_VOL_ICO_LARGE
    wxFS_VOL_ICO_SEL_SMALL
