typedef wxObjectListNode wxNode

%enum wxKeyType
    wxKEY_NONE
    wxKEY_INTEGER
    wxKEY_STRING
%end

%union wxListKeyValue
    %union wxListKeyValue
%end

///////////////////////////////////////////////////////////////////////////////
// wxListBase

%include "\list.h"
%class %noclassinfo wxListBase, wxObject
    wxListBase(wxKeyType keyType = wxKEY_NONE)
    virtual ~wxListBase()
    size_t GetCount() const
    bool IsEmpty() const
    void Clear()
    void DeleteContents(bool destroy)
    bool GetDeleteContents() const
    wxKeyType GetKeyType() const
    void SetKeyType(wxKeyType keyType)
    wxDEPRECATED( int Number() const )
    wxDEPRECATED( wxNode *First() const )
    wxDEPRECATED( wxNode *Last() const )
    wxDEPRECATED( wxNode *Nth(size_t n) const )
    wxDEPRECATED( operator wxList&() const )
    wxListBase(size_t count, void *elements[])
    wxListBase(void *object, ... )
    int wxListBase::Number() const
    wxNode *wxListBase::First() const
    wxNode *wxListBase::Last() const
    wxNode *wxListBase::Nth(size_t n) const
    wxListBase::operator wxList&() const
%endclass

///////////////////////////////////////////////////////////////////////////////
// wxNodeBase

%include "\list.h"
%class %noclassinfo wxNodeBase
    wxNodeBase(wxListBase *list = (wxListBase *)NULL,wxNodeBase *previous = (wxNodeBase *)NULL,wxNodeBase *next = (wxNodeBase *)NULL,void *data = NULL,const wxListKey& key = wxDefaultListKey)
    virtual ~wxNodeBase()
    const wxChar *GetKeyString() const
    long GetKeyInteger() const
    void SetKeyString(wxChar* s)
    void SetKeyInteger(long i)
    wxDEPRECATED( wxNode *Next() const )
    wxDEPRECATED( wxNode *Previous() const )
    wxDEPRECATED( wxObject *Data() const )
    void** GetDataPtr() const
    wxNode *wxNodeBase::Next() const
    wxNode *wxNodeBase::Previous() const
    wxObject *wxNodeBase::Data() const
%endclass

