    unsigned int GetPriority() const
    bool IsAlive() const
    bool IsRunning() const
    bool IsPaused() const
    bool IsDetached() const
    wxThreadIdType GetId() const
    virtual void OnExit()
    virtual bool TestDestroy()
    virtual ~wxThread()
%endclass

